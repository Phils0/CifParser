<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Phil Sharp</Authors>
    <Description>Processes a UK rail timetable file in the CIF format.  Works with both RDG and Netowrk Rail versions of the file.</Description>
    <PackageProjectUrl>https://github.com/Phils0/CifParser/</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/Phils0/CifParser/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>ukrail cif</PackageTags>
    <PackageVersion>0.0.0</PackageVersion>
    <Title>UK Rail Timetable CIF parser</Title>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FileHelpers" Version="3.4.1" />
    <PackageReference Include="Serilog" Version="2.9.0" />
  </ItemGroup>

</Project>
